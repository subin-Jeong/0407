 {{> board/include/top}}
<body id="page-top">
<!-- Page Wrapper -->
<div id="wrapper">
	<!-- Content Wrapper -->
	<div id="content-wrapper" class="d-flex flex-column">
		<!-- Main Content -->
		<div id="content">
			 {{> board/include/header}} 
			<!-- Begin Page Content -->
			<div class="container-fluid">
				<!-- DataTales Example -->
				<div class="card shadow mb-4">
					<div class="card-header py-3">
						<h6 class="m-0 font-weight-bold text-primary">
						<div style="float:right;">
							<a href="/board/write" class="btn btn-primary btn-icon-split">
							<span class="text">글등록</span>
							</a>
						</div>
						</h6>
					</div>
					<div class="card-body">
						<div>
							<table class="table table-bordered hover" id="boardList" width="100%" cellspacing="0">
							<thead>
							<tr>
								<th width="10%">
									 번호
								</th>
								<th width="55%">
									 제목
								</th>
								<th width="15%">
									 등록일자
								</th>
								<th width="10%">
									 작성자
								</th>
							</tr>
							</thead>
							</table>
						</div>
					</div>
				</div>
			</div>
			<!-- /.container-fluid -->
		</div>
		<!-- End of Main Content -->
		 {{> board/include/footer}}
	</div>
	<!-- End of Content Wrapper -->
</div>
<!-- End of Page Wrapper -->
 {{> board/include/scroll}} 

